{
    "name": "dvelum/dvelum-core",
    "version": "3.0.0-dev",
    "type": "dvelum3-extension",
    "description": "DVelum core classes",
    "keywords": ["dvelum"],
    "homepage": "https://github.com/dvelum/dvelum-core",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "email": "kirill@dvelum.com",
            "role": "Founder, Developer"
        },{
            "name": "Sergey Leschenko",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dvelum/dvelum/issues",
        "source": "https://github.com/dvelum/dvelum"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php":">=7.3.0",
        "ext-mysqli" : "*",
        "ext-mbstring" : "*",
        "ext-json": "*",
        "composer/installers": "~1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "zendframework/zend-db": "^2.9.1",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7",
        "phpstan/phpstan": "^0.10",
        "codacy/coverage": "dev-master"
    }
}
