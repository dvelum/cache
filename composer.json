{
    "name": "dvelum/cache",
    "version": "4.0.0",
    "type": "library",
    "description": "DVelum 4.x Cache extension",
    "keywords": ["dvelum","php","cache"],
    "homepage": "https://github.com/dvelum/cache",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Founder, Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dvelum/cache/issues",
        "source": "https://github.com/dvelum/cache"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php":">=7.4.0",
        "ext-memcached" : "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dvelum\\Cache\\": "src/"
        }
    }
}
