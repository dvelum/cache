{
    "name": "dvelum/dvelum-core",
    "version": "3.0.0",
    "type": "project",
    "description": "DVelum 3.x core",
    "keywords": ["dvelum","php"],
    "homepage": "https://github.com/dvelum/dvelum-core",
    "license": "MIT",
    "authors": [
        {
            "name": "Kirill Yegorov",
            "role": "Founder, Developer"
        },{
            "name": "Sergey Leschenko",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/dvelum/dvelum/issues",
        "source": "https://github.com/dvelum/dvelum-core"
    },
    "config": {
        "preferred-install": "dist",
        "classmap-authoritative": true
    },
    "require": {
        "php":">=7.3.0",
        "ext-mysqli" : "*",
        "ext-gd" : "*",
        "ext-mbstring" : "*",
        "ext-json": "*",
        "composer/installers": "~1.0",
        "oomphinc/composer-installers-extender": "^1.1",
        "zendframework/zend-db": "^2.9.1",
        "zendframework/zend-mail": "^2.8",
        "psr/log": "^1.0",
        "matthiasmullie/minify": "~1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^0.12",
        "codacy/coverage": "dev-master"
    }
}
